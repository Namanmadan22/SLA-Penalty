{
  "projects": {
    "frontend": {
      "src": "./frontend",
      "output": "/"
    },
    "backend": {
      "src": "./backend",
      "functions": "api"
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/frontend/formm.html" },
    { "source": "/api/(.*)", "destination": "/backend/api/$1" }
  ]
}
